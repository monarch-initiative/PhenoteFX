{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "frequency",
  "$id": "http://jax.org/common_disease_annotation/frequency",
  "description": "The field to indicate the frequency of some terms in disease annotation",
  "definitions": {
    "frequency": {
      "type": "object",
      "oneof":[
        {
          "properties": {
            "fraction": {
              "type": "object",
              "numerator": {"type": "integer"},
              "denominator": {"type": "integer"}
            }
          }
        },
        {
          "properties": {
            "range": {
              "type": "object",
              "properties": {
                "low": {"$ref": "#/definitions/fraction"},
                "high": {"$ref": "#/definitions/fraction"}
              }
            }
          }
        }
      ]
    }
  }
}