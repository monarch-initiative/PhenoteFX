{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "CurationMeta",
  "$id": "http://jax.org/common_disease_annotation/curationmeta",
  "description": "Model for curation meta data",
  "type": "object",
  "properties": {
    "created": {
      "type": "object",
      "properties": {
        "curator": {
          "type": "string",
          "minLength": 5
        },
        "time": {
          "type": "string",
          "format": "date-time"
        }
      }
    },

    "edited": {
      "type": "object",
      "properties": {
        "curator": {
          "type": "string",
          "minLength": 5
        },
        "time": {
          "type": "string",
          "format": "date-time"
        }
      }
    }

  }
}